use strict;
use warnings;
use Data::Dumper;
use DDP;

my @res = ();
while (<>) {
    my @tmp = split(';');
    $tmp[8] =~ s/\s+$//; #It is to delete \n at the end of filename. After RegExp lection i rewrote it :)
    push @res,  \@tmp;
}

print "Data::Dumper demo:\n";
print Dumper(@res);
print "Data::Printer demo:\n";
p @res
